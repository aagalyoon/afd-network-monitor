# Example environment configuration
# Copy this file to .env to set custom configurations

# Real API endpoints - configure these to point to your lab network endpoints
VITE_WHISKEY1_API=http://10.10.4.161:31800
VITE_WHISKEY2_API=http://10.10.4.162:31800

# Feature flags
VITE_ENABLE_NETWORK_TEST=true
VITE_SHOW_REAL_ERRORS=true

# Timeouts (in milliseconds)
VITE_API_TIMEOUT=10000
VITE_NETWORK_TEST_TIMEOUT=20000

